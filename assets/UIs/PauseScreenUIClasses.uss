.button {
    width: 300px;
    align-items: center;
    align-self: center;
    -unity-font-style: normal;
    color: rgb(0, 0, 0);
    font-size: 40px;
    display: flex;
    visibility: visible;
    -unity-background-image-tint-color: rgb(0, 0, 0);
    background-color: rgb(255, 255, 255);
    margin-top: 20px;
    margin-right: 20px;
    margin-bottom: 20px;
    margin-left: 20px;
    height: 80px;
}
